{
  "name": "archipel-app",
  "productName": "Archipel",
  "version": "2.0.1",
  "license": "MIT",
  "description": "Archipel Desktop App",
  "author": "Archipel:Somoco Team",
  "scripts": {
    "start": "cd electron && NODE_ENV=development electron .",
    "dev": "npm-run-all watch start",
    "build:prod": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "watch": "webpack --watch --mode=development",
    "pretest": "npm run lint",
    "test": "npm run test:unit",
    "test:unit": "cross-env NODE_ENV=test babel-tape-runner -r ./unit-tests/_helpers/*.js unit-tests/*.js",
    "lint": "standard",
    "deps": "cross-env DEBUG=* dependency-check . --detective precinct --entry app/index.js"
  },
  "standard": {
    "ignore": []
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.6",
    "dependency-check": "^3.1.0",
    "gaze": "^1.1.3",
    "html-webpack-plugin": "^3.2.0",
    "npm-run-all": "^4.1.3",
    "precinct": "^4.2.0",
    "rebass": "^1.0.7",
    "standard": "^11.0.1",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "chroma-js": "^1.3.7",
    "dnode": "^1.2.2",
    "hyper-graph-db": "^0.3.4",
    "is-electron-renderer": "^2.0.1",
    "palx": "^1.0.3",
    "react": "^16.4.0",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "styled-components": "^3.2.6",
    "thunky": "^1.0.2",
    "websocket-stream": "^5.1.2"
  }
}
