{
  "name": "archipel-app",
  "productName": "Archipel",
  "version": "2.0.1",
  "license": "MIT",
  "description": "Archipel Desktop App",
  "author": "Archipel:Somoco Team",
  "scripts": {
    "setup": "npm install && cd app && npm install && cd ../electron && npm install",
    "start": "NODE_ENV=development node server.js",
    "electron": "cd electron; NODE_ENV=development npm run start",
    "dev": "npm-run-all -p watch start electron",
    "watch": "cd app; npm run watch",
    "lint": "standard"
  },
  "standard": {
    "ignore": []
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3",
    "opn": "^5.3.0",
    "standard": "^11.0.1"
  },
  "dependencies": {
    "dnode": "^1.2.2",
    "electron-ipc-webview-stream": "^0.5.1",
    "hyper-graph-db": "^0.3.4",
    "multihyperdb": "0.0.2",
    "rpc-multistream": "^0.9.7",
    "through2": "^2.0.3",
    "websocket-stream": "^5.1.2"
  }
}
